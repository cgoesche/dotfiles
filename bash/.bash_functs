function printeradd(){
	PRINTERNAME=$1
	PRINTER_IP=$2
	lpadmin -p ${PRINTERNAME} -E -v ipp://${PRINTER_IP}/ipp/print -m everywhere
	
	if [ $? -ne 0 ] ; then
		echo "Printer ${PRINTERNAME} could not be added!"
		return 1
	fi
	return 0
}

